body {
	font-size: 14px;
	text-align: justify;
	word-wrap: normal;
	font-variant: normal;
	font-family: Helvetica, Verdana, Arial, sans-serif;
}

h1 {
	font-family: Helvetica, Verdana, Arial, sans-serif;
	font-variant: normal;
	color: #030371;
	font-size: 24px;
}

h2 {
	font-family: Helvetica, Verdana, Arial, sans-serif;
	color: #8792ff;
	font-size: 20px;
}

ul {
	margin-bottom: -1px;
	font-family: Helvetica, Verdana, Arial, sans-serif;
	font-size: 14px;
}

table {
	width: 60%;
	border-spacing: 16px 2px;
	background-color: #dadaff;
	border-width: 1px;
	border-style: none;
	font-size: 13px;
	font-family: Helvetica, Verdana, Arial, sans-serif;
}

tr {
	font-family: Helvetica, Verdana, Arial, sans-serif;
	border-width: 1px;
}

td {
	border-width: 1px;
}

div.tablefield {
	font-weight: bold;
}

span.highrisk {
	color: #ff0f06;
	font-weight: bold;
}

span.attackstring {
	font-family: monospace;
}

span.parameter {
	font-style: italic;
}

span.medrisk {
	color: #e24e28;
}

span.lowrisk {
	color: #01ad00;
}

span.title {
	font-weight: bold;
	bottom: 25px;
	position: relative;
	text-overflow: clip;
	color: #020589;
	font-family: Helvetica, Verdana, Arial, sans-serif;
	font-size: 38px;
}

div.reflink {
	line-height: 28px;
	text-transform: none;
}

a {
	text-decoration: none;
	font-family: Helvetica, Verdana, Arial, sans-serif;
	color: #000daf;
	font-variant: normal;
}

a.visited {
	text-decoration: none;
}

div.section {
	padding-bottom: 8px;
	padding-top: 8px;
	border-width: 1px;
	border-style: none;
}

div.topsection {
	border-style: none;
}

a.reflink {
	font-size: 12px;
	color: #4c00ff;
	text-decoration: none;
}

a.innerlink {
	color: #4c00ff;
	text-decoration: none;
}

a.innerlinksmall {
	font-size: 12px;
	color: #4c00ff;
	text-decoration: none;
}
